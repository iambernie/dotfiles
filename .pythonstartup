#!/usr/bin/env python

try:
    import os
    import sys
    print("imported os")
    print("imported sys")
except ImportError: pass
    

try:
    import numpy as np
    np.set_printoptions(threshold=np.nan, linewidth=120)
    print("imported numpy as np")
except ImportError: pass


try:
    import matplotlib as mpl
    import matplotlib.pyplot as plt
    print("imported matplotlib as mpl")
    print("imported matplotlib.pyplot as plt")
except ImportError: pass


try:
    import h5py
    print("imported h5py")
except ImportError: pass

try:
    import sympy
    import sympy.symbols as symbols
    print("imported sympy")
    print("imported sympy.symbols as symbols")
except ImportError: pass

try:
    from amuse.units import units
    from amuse.datamodel.particles import Particles
    print("imported amuse.units.units as units")
    print("imported amuse.datamodel.particles.Particles as Particles")
except ImportError: pass

try:
    from jedi.utils import setup_readline
    setup_readline()
except ImportError:
    # Fallback to the stdlib readline completer if it is installed.
    # Taken from http://docs.python.org/2/library/rlcompleter.html
    print("Jedi is not installed, falling back to readline")
    try:
        import readline
        import rlcompleter
        readline.parse_and_bind("tab: complete")
    except ImportError:
        print("Readline is not installed either. No tab completion is enabled.")


try:
    from pprint import pprint as pp
    print("imported pprint from pprint as pp")
except ImportError: pass

