#!/usr/bin/env python

try:
    import os
    import sys
    print("import os")
    print("import sys")
except ImportError: pass
    

try:
    import numpy as np
    np.set_printoptions(threshold=np.nan, linewidth=120)
    print("import numpy as np")
except ImportError: pass


try:
    import matplotlib as mpl
    import matplotlib.pyplot as plt
    print("import matplotlib as mpl")
    print("import matplotlib.pyplot as plt")
except ImportError: pass


try:
    import h5py
    print("import h5py")
except ImportError: pass

try:
    import sympy
    from sympy import symbols
    print("import sympy")
    print("from sympy import symbols")
except ImportError: pass

try:
    from amuse.units import units
    from amuse.datamodel.particles import Particles
    print("from amuse.units import units")
    print("from amuse.datamodel.particles import Particles")
except ImportError: pass

try:
    from jedi.utils import setup_readline
    setup_readline()
    from jedi import settings
    settings.case_insensitive_completion = False
    print("loaded jedi")
except ImportError:
    # Fallback to the stdlib readline completer if it is installed.
    # Taken from http://docs.python.org/2/library/rlcompleter.html
    print("Jedi is not installed, falling back to readline")
    try:
        import readline
        import rlcompleter
        readline.parse_and_bind("tab: complete")
    except ImportError:
        print("Readline is not installed either. No tab completion is enabled.")


try:
    from pprint import pprint as pp
    print("from pprint import pprint as pp")
except ImportError: pass

